{
  "name": "ai-tech-nexus",
  "services": [
    {
      "name": "web",
      "type": "nodejs",
      "version": "18.17.0",
      "build": {
        "command": "npm install && npm run build",
        "outputDir": "dist"
      },
      "run": {
        "command": "npm run preview",
        "port": 3000
      },
      "envs": {
        "NODE_ENV": "production",
        "PATH": "/usr/local/bin:/usr/bin:/bin"
      }
    }
  ]
}